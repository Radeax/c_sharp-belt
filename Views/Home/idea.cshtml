@model List<Like>

<table>
    <tr>
        <td>@ViewBag.Creator says: </td>
        <td>@ViewBag.Idea</td>
    </tr>
</table>
<hr>
<table>
    <tr>
        <th colspan="2">People who liked this post:</th>
    </tr>
    <tr>
        <th>Alias</th>
        <th>Name</th>
    </tr>
    @foreach (Like like in Model)
    {
        <tr>
            <td>@like.User.Alias</td>
            <td>@like.User.Name</td>
        </tr>
    }
</table>